src_unpack() {
	cargo_src_unpack

	cd "${S}" || die
	rmdir libs/hbb_common || die
	mv "${WORKDIR}/hbb_common-${HBB_COMMON_COMMIT}" libs/hbb_common || die

	local webm_sys_dir="${WORKDIR}/rust-webm-d2c4d3ac133c7b0e4c0f656da710b48391981e64/src/sys"
	rmdir "${webm_sys_dir}/libwebm" || die
	mv "${WORKDIR}/libwebm-libwebm-${LIBWEBM_VER}" "${webm_sys_dir}/libwebm" || die

	local kcp_sys_dir="${WORKDIR}/kcp-sys-32a6c09fc6223f54aea83981a6aa8995931d29be"
	rmdir "${kcp_sys_dir}/kcp" || die
	mv "${WORKDIR}/kcp-${KCP_COMMIT}" "${kcp_sys_dir}/kcp" || die
}

src_prepare() {
	default

	# Remove hwcodec feature references
	sed -i -e '/^hwcodec/d' -e '/\["hwcodec"\]/d' -e 's/"hwcodec",\?//g' \
		libs/scrap/Cargo.toml libs/hbb_common/Cargo.toml Cargo.toml || die

	# Enable linux-pkg-config feature which includes both scrap and magnum-opus
	sed -i 's/default = \[/default = ["linux-pkg-config", /' \
		Cargo.toml || die

	# Apply patch to sciter-rs (fix UB in legacy code)
	pushd "${WORKDIR}/rust-sciter-5322f3a755a0e6bf999fbc60d1efc35246c0f821" >/dev/null || die
	eapply "${FILESDIR}/sciter-fix-ub.patch"
	popd >/dev/null || die

	# Patch scrap build.rs to use dynamic linking instead of static
	eapply "${FILESDIR}/scrap-dynamic-linking.patch"

	# Patch UI path to use system location
	eapply "${FILESDIR}/rustdesk-ui-system-path.patch"
}

src_configure() {
	cargo_src_configure
}

src_compile() {
	# Build system expects vcpkg for some libraries without .pc files
	# Create fake vcpkg pointing to system libraries instead
	export NO_PKG_CONFIG_libyuv=1
	export NO_PKG_CONFIG_libvpx=1
	export NO_PKG_CONFIG_aom=1
	export VCPKG_ROOT="${WORKDIR}/fake-vcpkg"
	export VCPKG_INSTALLED_ROOT="${VCPKG_ROOT}/installed"

	mkdir -p "${VCPKG_ROOT}/installed/x64-linux/"{lib,include} || die
	ln -sf /usr/$(get_libdir)/libyuv.so "${VCPKG_ROOT}/installed/x64-linux/lib/libyuv.so" || die
	ln -sf /usr/$(get_libdir)/libvpx.so "${VCPKG_ROOT}/installed/x64-linux/lib/libvpx.so" || die
	ln -sf /usr/$(get_libdir)/libaom.so "${VCPKG_ROOT}/installed/x64-linux/lib/libaom.so" || die
	ln -sfn /usr/include/libyuv "${VCPKG_ROOT}/installed/x64-linux/include/libyuv" || die
	ln -sfn /usr/include/vpx "${VCPKG_ROOT}/installed/x64-linux/include/vpx" || die
	ln -sfn /usr/include/aom "${VCPKG_ROOT}/installed/x64-linux/include/aom" || die

	cargo_src_compile
}

src_install() {
	local rustdesk_dir="/usr/share/${PN}"

	exeinto "${rustdesk_dir}"
	doexe $(cargo_target_dir)/rustdesk

	insinto "${rustdesk_dir}"
	newins "${DISTDIR}/libsciter-gtk-${PV}.so" libsciter-gtk.so

	insinto "${rustdesk_dir}/ui"
	doins -r src/ui/*

	dobin "${FILESDIR}/rustdesk.sh"
	mv "${ED}/usr/bin/rustdesk.sh" "${ED}/usr/bin/rustdesk" || die

	newicon -s 32 res/32x32.png rustdesk.png
	newicon -s 128 res/128x128.png rustdesk.png
	newicon -s 256 res/128x128@2x.png rustdesk.png

	domenu "${FILESDIR}"/rustdesk{,-link}.desktop

	einstalldocs
}
